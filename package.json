{
  "name": "stenimator",
  "version": "0.9.3",
  "description": "Animator for Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stenimator/stenimator.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randy-r/stenimator.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "keywords": [
    "stencil",
    "stenciljs",
    "animate",
    "page",
    "animations",
    "transitions"
  ],
  "scripts": {
    "build": "rm -fR ./dist && stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "2.18.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0",
    "stencil-router-v2": "^0.6.0"
  },
  "author": "randy-r",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/randy-r/stenimator/issues"
  },
  "homepage": "https://randy-r.github.io/stenimator/"
}
